int mySqrt(int x) {
   
    long  s=0,e=x,m,r=0;
    while(s<=e){
        m=(s+e)/2;
        
        if(m*m==x){
            r=m;
            break;
        }
        else if(m*m<x){
            s=m+1;
            r=m;
            
        }
        else{
            e=m-1;
        }
    }
    return r;

}
