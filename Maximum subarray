int maxSubArray(int* nums, int numsSize) {
    int max=nums[0];
    int c=nums[0];
    for(int i=1;i<numsSize;i++){
        if(c+nums[i]>nums[i]){
            c+=nums[i];
        }
        else{
            c=nums[i];
        }
        if(c>max){
            max=c;
        }
    }
    return max;
}
